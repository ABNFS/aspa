[{% for account in accounts %}
 {
  "id": {{ account.id }},
  "code": "{{ account.code }}",
  "balance": {{ account.balance }},
  {%if account.alias %}"alias": "{{ account.alias }}",{% endif %}
  {% if account.parent %}"parent": {{ account.parent }},{% endif %}
  "name": "{{ account.name}}",
  "accont_type": {{ account.account_type }},
  "currency": {{ account.currency }},
  {% if account.operate %}"operate": true{% else %}"operate": false{% endif %},
  "children": [{% for children in account.children%}{{ children.id }}{% if not loop.last %},{% endif %}{% endfor %}]
 {% if loop.last %}}{% else %}},{% endif %}
{% endfor %}]