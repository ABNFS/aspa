[
  {% for conta in contas %}
  {
  "codigo": "{{ conta. codigo }}",
  "saldo": {{ conta.saldo }},
  {%if conta.sigla %}
  "sigla": "{{ conta.sigla }}",
  {% endif %}
  {% if conta.pai %}
  "pai": {{ conta.pai }},
  {% endif %}
  "nome": "{{ conta.nome}}",
  "tipoConta": {{ conta.tipo_conta }},
  "moedaConta": {{ conta.moeda }},
  {% if conta.pode_movimentar %}
  "podeMovimentar": true
  {% else %}
  "podeMovimentar": false
  {% endif %}
  {% if loop.last %}
   }
  {% else %}
   },
  {% endif %}
 {% endfor %}
]